{
    "name": "salamwaddah/laravel-canonical",
    "description": ":package_description",
    "keywords": [
        "laravel",
        "canonical",
        "seo"
    ],
    "homepage": "https://github.com/salamwaddah/laravel-canonical",
    "license": "MIT",
    "authors": [
        {
            "name": "Salam Waddah",
            "email": "salamwaddah@gmail.com",
            "homepage": "https://salamwaddah.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },

    "autoload": {
        "psr-4": {
            "SalamWaddah\\LaravelCanonical\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SalamWaddah\\LaravelCanonical\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^2.33"
    }
}
